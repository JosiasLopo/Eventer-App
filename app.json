{
  "expo": {
    "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app allows you to upload photos from your library",
            "cameraPermission": "The app needs permission to access your camera"
          }
        ]
      ],
    "name": "eventer",
    "slug": "eventer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101014"
    },
    "assetBundlePatterns": ["**/*"],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#101014"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}